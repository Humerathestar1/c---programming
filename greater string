//greater string
#include <iostream>
#include <string>  
using namespace std;

int main() {
    string str1, str2;
    cout << "Enter first string: ";
    getline(cin, str1);
    cout << "Enter second string: ";
    getline(cin, str2);
    if (str1 > str2) {
        cout << "The greater string is: " << str1 << endl;
    } else if (str1 < str2) {
        cout << "The greater string is: " << str2 << endl;
    } else {
        cout << "Both strings are equal." << endl;
    }

    return 0;
}
